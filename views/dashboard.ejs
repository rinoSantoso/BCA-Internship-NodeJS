      labels: pdrbYearlyLabels,
      datasets: [{
        label: "PDRB Total",
        data: pdrbYearlyDataValues,
        fill: false,
        borderColor: '#efefef',
        tension: 0.1,
      },]
    }

    const pdrbYearlyConfig = {
      type: 'line',
      data: pdrbYearlyData,
      options: {  
        responsive: true,
        
        interaction: {
          mode: 'index',
          intersect: false,
        },
        
        stacked: false,
        
        plugins: {
          title: {
            display: false,
            text: 'Laba/Rugi vs. PDRB'
          },
          legend: {
            display: false
          },
          tooltip: {
            callbacks: {
              label: function(context) {
                let label = context.dataset.label || '';

                  if (label) {
                    label += ': ';
                  }
                  if (context.parsed.y !== null) {
                    if(context.parsed.y >= 1000000000000 || context.parsed.y <= -1000000000000){
                      label += new Intl.NumberFormat('en-US', { style: 'currency', currency: 'IDR' }).format(context.parsed.y / 1e12) + 'T';
                    }
                    
                  }
                  return label;
              }         
            }
          }
        },

        scales: {
          x: {
            grid:{
              color: 'rgba(239,239,239,0.8)',
              borderDash: [2,2],
            },
            
            ticks: {
              color: 'rgba(239,239,239,0.8)',
            }

          },
          
          y: {
            ticks: {
              callback: function(value, index, values) {
                if (value >= 1000000000000 || value <= -1000000000000) {
                  return value / 1e12 + 'T';
                }
